	keywords = {Atlantic menhaden, ecosystem-based fisheries management, multispecies virtual population analysis, trophic models},
	pages = {856--870},
	file = {Full Text PDF:/Users/okenk/Library/Application Support/Firefox/Profiles/onkxf6d5.default-1399309436514/zotero/storage/GM7WZG4D/Garrison et al. - 2010 - An expansion of the MSVPA approach for quantifying.pdf:application/pdf;Snapshot:/Users/okenk/Library/Application Support/Firefox/Profiles/onkxf6d5.default-1399309436514/zotero/storage/DNB5Z6CF/856.html:text/html}
