	journal = {Proceedings of the National Academy of Sciences},
	author = {Schindler, David W. and Hecky, R. E. and Findlay, D. L. and Stainton, M. P. and Parker, B. R. and Paterson, M. J. and Beaty, K. G. and Lyng, M. and Kasian, S. E. M.},
	month = aug,
	year = {2008},
	pmid = {18667696},
	keywords = {cyanobacteria blooms, Experimental Lakes, nutrient limitation, phosphorus},
	pages = {11254--11258},
	file = {Full Text PDF:/Users/okenk/Library/Application Support/Firefox/Profiles/onkxf6d5.default-1399309436514/zotero/storage/7JM2DR9S/Schindler et al. - 2008 - Eutrophication of lakes cannot be controlled by re.pdf:application/pdf;Snapshot:/Users/okenk/Library/Application Support/Firefox/Profiles/onkxf6d5.default-1399309436514/zotero/storage/QGIA983W/11254.html:text/html}
