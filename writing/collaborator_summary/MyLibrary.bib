	file = {Full Text PDF:/Users/okenk/Library/Application Support/Firefox/Profiles/onkxf6d5.default-1399309436514/zotero/storage/JFNRC29B/Cheung et al. - 2010 - Large-scale redistribution of maximum fisheries ca.pdf:application/pdf;Snapshot:/Users/okenk/Library/Application Support/Firefox/Profiles/onkxf6d5.default-1399309436514/zotero/storage/E5HDMX78/full.html:text/html}
